{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "jasmine": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
      "prettier/prettier": "error",
      "react/jsx-one-expression-per-line": "off"
    }
}
